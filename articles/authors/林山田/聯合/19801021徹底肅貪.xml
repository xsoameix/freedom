@8 21600 0"/><v:f eqn="prod @7 21600 pixelHeight"/><v:f eqn="sum @10 21600 0"/></v:formulas><v:path o:extrusionok="f" gradientshapeok="t" o:connecttype="rect"/><o:lock v:ext="edit" aspectratio="t"/></v:shapetype><v:shape id="_x0000_i1025" type="#_x0000_t75" alt="" style="width:1.5pt;height:8.25pt"><v:imagedata r:id="rId4" r:href="rId5"/></v:shape></w:pict></w:r><w:r w:rsidRPr="00950C60"><w:rPr><w:rFonts w:ascii="新細明體" w:hAnsi="新細明體" w:cs="新細明體"/><w:color w:val="000000"/><w:kern w:val="0"/></w:rPr><w:fldChar w:fldCharType="end"/></w:r></w:p><w:p w:rsidR="00950C60" w:rsidRPr="00950C60" w:rsidRDefault="00950C60" w:rsidP="00950C60"><w:pPr><w:widowControl/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="000000"/><w:kern w:val="0"/></w:rPr></w:pPr><w:r w:rsidRPr="00950C60"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="000000"/><w:kern w:val="0"/></w:rPr><w:t>【林山田】</w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:rPr><w:rFonts w:ascii="新細明體" w:hAnsi="新細明體" w:cs="新細明體"/><w:vanish/><w:kern w:val="0"/></w:rPr></w:pPr></w:p><w:tbl><w:tblPr><w:tblW w:w="4900" w:type="pct"/><w:jc w:val="center"/><w:tblCellSpacing w:w="37" w:type="dxa"/><w:tblCellMar><w:top w:w="75" w:type="dxa"/><w:left w:w="75" w:type="dxa"/><w:bottom w:w="75" w:type="dxa"/><w:right w:w="75" w:type="dxa"/></w:tblCellMar><w:tblLook w:val="0000" w:firstRow="0" w:lastRow="0" w:firstColumn="0" w:lastColumn="0" w:noHBand="0" w:noVBand="0"/></w:tblPr><w:tblGrid><w:gridCol w:w="8432"/></w:tblGrid><w:tr w:rsidR="00E339A7" w:rsidRPr="00E339A7"><w:trPr><w:tblCellSpacing w:w="37" w:type="dxa"/><w:jc w:val="center"/></w:trPr><w:tc><w:tcPr><w:tcW w:w="0" w:type="auto"/><w:vAlign w:val="center"/></w:tcPr><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>大里鄉非法變更地目案，經台中地方法院初審判決，涉案的公務員，四人處死刑，六人處無期徒刑，一人處十五年有期徒刑，本案雖未定蠵，但就量刑的輕重來看，已在我國刑事司法對於貪污罪的審判史上創了紀錄。</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve"> </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>對於尚未定讞的刑案，不應評論；即使已然定蠵只要司法官依法行使審判的職議，亦具有獺立超然的裁量權，外界的評論，不足以影響審判的結果。</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve"> </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>一般人對本案初審判決的反應，就目前所知，均在量刑輕重的問題上，但這個問題如前面指出，實是司法官獨立審判的職蠵，不宜有所評蠵；但由於一般人對於此一角度較感興趣，亦可以超越對於本案的個案討論，從一個比較超然的立場，來看這個問題。</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve"> </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>那麼，司法判決的輕重，究竟是否可以影響肅貧效果的得失？玆就此一問題，表示個人的看法。</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve"> </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve">刑罰輕重對於肅貪的效果，或許會有一些影響，但在理論上，重罰並不一定是最有效的肅貪方法。因為，刑罰再重，如果不能提高貪污事件的追訴率，或一般通稱的破獲率，漏網之魚一多，恐怕就難收到整飭吏治的預期效果，僥倖者一樣能逍遙於法律之外，享受其貪污的「成果」。　</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve"> </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve">食污可以說是「犯罪黑數」最高的一種犯罪形態。依據現行法的規定來說，行賄者與受賄譽同樣要受處罰，在這種相同的利害關係下，貪污事件的嫂證工作頗有困蠵；依過去的有關統計來看，各地方法院檢察處所偵察的貪污案件，約有三分之一不起訴；在起訴的案件中，又有三分之一在地方法院判決無罪，足見追訴與譽判此類案件的困難。由於這類因素，貪污犯罪的「黑數」即可能存在，雖有重罰，亦無法發伏揭隱，予犯罪譽以應得的處罰。因此，一二個案的重罰，即很蠵發生肅貪的效果。　</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve"> </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>個人認為，罰不須重，只要能持續而穩定地增加並維持貪污事件的破獲率或追訴率，不要時蠵時緊，一曝十寒，即能遏止貪墨的事件，換句話說，肅貪工作，在司法院上所能發揮的最大功能，應在有效地降低此獺犯罪的「黑數」。</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>(</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>本文作者現任輔仁大學教授</w:t></w:r><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t xml:space="preserve">) </w:t></w:r></w:p><w:p w:rsidR="00E339A7" w:rsidRPr="00E339A7" w:rsidRDefault="00E339A7" w:rsidP="00E339A7"><w:pPr><w:widowControl/><w:spacing w:before="100" w:beforeAutospacing="1" w:after="100" w:afterAutospacing="1" w:line="360" w:lineRule="atLeast"/><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr></w:pPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>【</w:t></w:r><w:smartTag w:uri="urn:schemas-microsoft-com:office:smarttags" w:element="chsdate"><w:smartTagPr><w:attr w:name="IsROCDate" w:val="False"/><w:attr w:name="IsLunarDate" w:val="False"/><w:attr w:name="Day" w:val="21"/><w:attr w:name="Month" w:val="10"/><w:attr w:name="Year" w:val="1980"/></w:smartTagPr><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="Verdana" w:hAnsi="Verdana" w:cs="新細明體"/><w:color w:val="414141"/><w:kern w:val="0"/><w:sz w:val="22"/><w:szCs w:val="22"/></w:rPr><w:t>1980-10-21</w:t></w:r></w:smartTag><w:r w:rsidRPr="00E339A7"><w:rPr><w:rFonts w:ascii="